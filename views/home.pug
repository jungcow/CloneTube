extends layout/main
include mixins/videoBlock

block content
  .video-container
    each video, index in videos
      +videoBlock({
        id: video.id,
        fileUrl: video.fileUrl,
        title: video.title,
        views: video.views,
        uploadedAt: uploadedArray[index],
        creatorId : video.creator.id,
        creatorName: video.creator.name,
        creatorAvatarUrl: video.creator.avatarUrl
      })

  